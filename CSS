@keyframes confetti-fall {
    to {
        transform: translateY(100vh) rotate(360deg);
    }
}

.confetti {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    overflow: hidden;
    pointer-events: none;
    z-index: 9999;
    background: rgba(0, 0, 0, 0.5);
}

.confetti::before,
.confetti::after {
    content: '';
    position: absolute;
    width: 100%;
    height: 100%;
    background: radial-gradient(circle, rgba(255, 223, 186, 0.6), rgba(255, 223, 186, 0) 70%),
                radial-gradient(circle, rgba(224, 164, 138, 0.6), rgba(224, 164, 138, 0) 70%),
                radial-gradient(circle, rgba(140, 206, 203, 0.6), rgba(140, 206, 203, 0) 70%),
                radial-gradient(circle, rgba(225, 137, 163, 0.6), rgba(225, 137, 163, 0) 70%);
    background-size: 10px 10px, 10px 10px, 10px 10px, 10px 10px;
    background-position: 0 0, 20px 20px, 40px 40px, 60px 60px;
    opacity: 0.7;
    animation: confetti-fall 3s linear infinite;
}

.confetti::before {
    background-size: 15px 15px, 15px 15px, 15px 15px, 15px 15px;
    animation-duration: 4s;
}

.confetti::after {
    background-size: 12px 12px, 12px 12px, 12px 12px, 12px 12px;
    animation-duration: 5s;
}

.confetti div {
    position: absolute;
    width: 10px;
    height: 10px;
    background: rgba(255, 223, 186, 0.8);
    animation: confetti-fall 3s linear infinite;
}

.confetti div:nth-child(odd) {
    background: rgba(224, 164, 138, 0.8);
    animation-duration: 4s;
}

.confetti div:nth-child(even) {
    background: rgba(140, 206, 203, 0.8);
    animation-duration: 5s;
}

.confetti div:nth-child(3n) {
    background: rgba(225, 137, 163, 0.8);
}

.header {
    position: relative;
    overflow: hidden;
}

/* Ensure the confetti element is above all other content */
header {
    position: relative;
}

header .confetti {
    z-index: 1000;
}

/* Add some variety to the confetti */
@keyframes confetti-fall {
    0% {
        transform: translateY(-100vh) rotate(0deg);
    }
    100% {
        transform: translateY(100vh) rotate(360deg);
    }
}

